<?xml version="1.0"?>
<package format="3">
  <name>traj_opt_ros</name>
  <version>0.0.0</version>
  <description>
    The traj_opt_ros package
  </description>
  <license>
    Apache License, Version 2.0
  </license>
  <author email="astrobee-fsw@nx.arc.nasa.gov">
    Astrobee Flight Software
  </author>
  <maintainer email="astrobee-fsw@nx.arc.nasa.gov">
    Astrobee Flight Software
  </maintainer>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>rviz2</build_depend>
  <build_depend>rclcpp</build_depend>
  <build_depend>traj_opt_basic</build_depend>
  <build_depend>traj_opt_msgs</build_depend>
  <build_depend>ff_util</build_depend>
  <build_depend>ff_msgs</build_depend>

  <exec_depend>rviz2</exec_depend>
  <exec_depend>rclcpp</exec_depend>
  <exec_depend>traj_opt_basic</exec_depend>
  <exec_depend>traj_opt_msgs</exec_depend>
  <exec_depend>ff_util</exec_depend>
  <exec_depend>ff_msgs</exec_depend>

  <export>
    <rviz2 plugin="${prefix}/nodelet_plugins.xml"/>
  </export> 
</package>
